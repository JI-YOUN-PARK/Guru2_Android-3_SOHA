<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.memo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".HabitActivity"></activity>
        <activity android:name=".DiaryActivity"></activity>

        <activity
            android:name=".note.EditNote"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MemoActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".note.addNote"
            android:label="@string/title_activity_add_note"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".note.Note_detail"
            android:label="@string/title_activity_note_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>